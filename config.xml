<?xml version='1.0' encoding='utf-8'?>
<widget id="coffee.gaius.project-keeper" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Project Keeper</name>
    <description />
    <author email="gaius.coffee@gmail.com" href="http://gaius.coffee">
        Gaius Coffee
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.2" />
    <engine name="browser" spec="~4.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
